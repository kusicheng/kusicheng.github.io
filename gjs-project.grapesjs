+ l + '%)';\n        }\n\n        function clamp(n, min, max) {\n          return Math.max(min, Math.min(max, n));\n        }\n\n        function updateBg() {\n          var maxScroll = document.documentElement.scrollHeight - window.innerHeight;\n          var p = maxScroll > 0 ? window.scrollY / maxScroll : 0;\n          var hue1 = (210 + 120 * p) % 360;\n          var hue2 = (252 + 160 * p) % 360;\n          var hue3 = (280 + 200 * p) % 360;\n          document.documentElement.style.setProperty('--bg1', hsl(Math.round(hue1), 85, 18));\n          document.documentElement.style.setProperty('--bg2', hsl(Math.round(hue2), 82, 16));\n          document.documentElement.style.setProperty('--bg3', hsl(Math.round(hue3), 78, 14));\n\n          var y = window.scrollY;\n          var pos1x = 10 + 6 * Math.sin(y / 500);\n          var pos2x = 90 - 6 * Math.cos(y / 600);\n          var pos3y = 100 - 8 * Math.sin(y / 700);\n          document.body.style.backgroundPosition = pos1x + '% 10%, ' + pos2x + '% 20%, 50% ' + pos3y + '%, 0 0';\n        }\n        updateBg();\n        window.addEventListener('scroll', updateBg, {\n          passive: true\n        });\n        window.addEventListener('resize', updateBg);\n      })();\n    "}]},{"type":"script","name":"Responsive Image Loader","components":[{"type":"textnode","content":"\n      (function() {\n        var imgs = Array.prototype.slice.call(document.querySelectorAll('.js-responsive-img'));\n\n        function pickSrc(img) {\n          var w = window.innerWidth;\n          var src = img.getAttribute('data-src-desktop');\n          if (w <= 480) src = img.getAttribute('data-src-mobile');\n          else if (w <= 992) src = img.getAttribute('data-src-tablet');\n          if (img.getAttribute('src') !== src) img.setAttribute('src', src);\n        }\n\n        function hydrate() {\n          imgs.forEach(pickSrc);\n        }\n        hydrate();\n        var to;\n        window.addEventListener('resize', function() {\n          clearTimeout(to);\n          to = setTimeout(hydrate, 120);\n        });\n      })();\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","name":"Head Metadata","components":[{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Portfolio"}]},{"tagName":"meta","void":true,"attributes":{"charset":"UTF-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"meta","void":true,"attributes":{"name":"robots","content":"index,follow"}},{"tagName":"meta","void":true,"attributes":{"name":"generator","content":"GrapesJS Studio"}},{"tagName":"link","void":true,"attributes":{"rel":"preconnect","href":"https://fonts.googleapis.com"}},{"tagName":"link","void":true,"attributes":{"rel":"preconnect","href":"https://fonts.gstatic.com","crossorigin":""}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;600;700&family=Plus+Jakarta+Sans:wght@700&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"RgqnpsPIuEGk9I1p","minHeight":"500px"}],"id":"tnGDtJ1hjPK68tEG"}],"symbols":[],"custom":{"projectType":"web","id":""}}